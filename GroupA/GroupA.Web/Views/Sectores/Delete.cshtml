@model GroupA.Dominio.Sector

@{
    ViewBag.Title = "Delete";
}

<h2 class="text-center letraFormal">Eliminar Sector</h2>

<h3 class="letraPiola text-capitalize text-danger text-center">¿Seguro que quiere eliminar a este Sector?</h3>
<div>
    <h4>Sector</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Nombre)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Nombre)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Borrar" class="btn btn-danger active" /> |
            @Html.ActionLink("Volver a Sectores", "Index")
        </div>
    }
</div>

<link href="~/Content/FondosParaLetras.css" rel="stylesheet" />