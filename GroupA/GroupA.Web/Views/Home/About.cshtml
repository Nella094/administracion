@{
    ViewBag.Title = "Nosotros";
    ViewBag.Antonella = "Antonella D'Arpino";
    ViewBag.Franco = "Franco Tarchini";
    ViewBag.Ivan = "Ivan Taddei";
    ViewBag.Chelo = "Marcelo de Jesús Nuñez";
    
}
<h1>@ViewBag.Title.</h1>


<div class="container text-center" >
    <h1 class="letraPiola">
        Conoce al Equipo
    </h1>
    
    <div class="row">
        <div class="col-md-3">
            <div class="card">
                <img class="card-img-top img-circle img-responsive " src="https://i.ibb.co/g4cn8q2/Antonella.jpg" alt="">
                <div class="card-body">
                    <h2 class="card-title text-center">@ViewBag.Antonella</h2>
                    <p class="card-text">CEO de Group A</p>
                    <h4>Curriculum</h4>
                    <p class="card-text">@ViewBag.LoremIpsum</p>
                </div>
            </div>
        </div>
        <div class="col-md-3">
            <img class="card-img-top img-circle img-responsive" src="https://cdn.discordapp.com/attachments/616084495136981012/649040958549262336/unknown.png" alt="">
            <div class="card-body">
                <h2 class="card-title text-center">@ViewBag.Ivan</h2>
                <p class="card-text">Solution Designer de Group A</p>
                <h4>Curriculum</h4>
                <p class="card-text">@ViewBag.LoremIpsum</p>
            </div>
        </div>
        <div class="col-md-3">
            <img class="card-img-top img-circle img-responsive" src="https://imagizer.imageshack.com/a/img922/6184/tZFIiG.png" alt="">
            <div class="card-body">
                <h2 class="card-title text-center">@ViewBag.Franco</h2>
                <p class="card-text">FrontEnd Engineer de Group A</p>
                <h4>Curriculum</h4>
                <p class="card-text">@ViewBag.LoremIpsum</p>
            </div>
        </div>
        <div class="col-md-3">
            <img class="card-img-top img-circle img-responsive" src="https://i.ibb.co/QM960vb/Perfil.jpg" alt="">
            <div class="card-body">
                <h2 class="card-title text-center">@ViewBag.Chelo</h2>
                <p class="card-text">BackEnd Engineer de Group A</p>
                <h4>Curriculum</h4>
                <p class="card-text">@ViewBag.LoremIpsum</p>
            </div>
        </div>
    </div>
</div>
<hr />
<div class="container">
    <div class="row">
        <div class="col-md-12">
            <h1 class="text-center">Organigrama de la Empresa</h1>
            <img  class="img-responsive" src="https://cdn.discordapp.com/attachments/460218161938169867/649036372866236426/Oreganigrama_GROUPA.png" alt="ORGANIGRAMA-group-A" width="5000" height="1200" border="0">
        </div>
    </div>
</div>


